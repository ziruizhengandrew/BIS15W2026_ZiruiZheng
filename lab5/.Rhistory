names(homerange)
homerange %>%
select(class, mean.hra.m2)%>%
filter(class== "aves" | class == "mammalia")%>%
arrange(mean.hra.m2)
homerange %>%
select(class, mean.hra.m2)%>%
filter(class== "aves" | class == "mammalia")%>%
arrange(desc(mean.hra.m2))
